   "source": [
    "import torch\n",
    "import math\n",
    "\n",
    "mu = torch.randn(100, 1)\n",
    "sigma = torch.randn(100, 1).clamp(min=0.01)\n",
    "\n",
    "dist = torch.distributions.Normal(mu, sigma)\n",
    "entropy = dist.entropy().mean().item()\n",
    "\n",
    "entropy"
   ]
